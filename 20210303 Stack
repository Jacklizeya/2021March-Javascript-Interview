class Stack{
constructor() 
{
    this._array = []
}

//can only accept one value
push(newvalue) {this._array.push(newvalue); return this._array.length}  // no return
pop(newvalue) {this._array.pop(newvalue); return newvalue} //return the value pop
}

let stack = new Stack()

stack.push(5)
stack.push(2)
//console.log(stack.push(100))
//console.log(stack.pop(1000))

console.log(stack)
//console.log(stack._array)
